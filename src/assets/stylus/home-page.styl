.app-home-page {
  header {
    display flex
    justify-content center
    align-items center
    padding 7rem
    h1 {
      color rgb(32, 35, 41)
      font-size 6rem
    }
  }
  section {
    background-color rgb(39, 43, 51)
    min-height 65vh
    .gallery {
      display flex
      flex-wrap wrap
      justify-content center
      gap 2rem
      padding 3rem
    }
  }
}